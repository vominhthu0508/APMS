@using XT.Web.External

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="google" value="notranslate" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    @Styles.Render("~/Content/Stylesheets/layoutadmin")
    @if (IsSectionDefined(@"Styles"))
    {
        @RenderSection(@"Styles", false)
    }
</head>
<body class="skin-blue sidebar-mini wysihtml5-supported">
    <div id="wrapper" class="wrapper">
        @Html.Partial("../Layout/LayoutAdmin_Header")
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="/images/avatar04.png" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>@Html.GetResource("Layout_LayoutAdmin__line51")</p>
                        <a href="#"><i class="fa fa-circle text-success"></i>@Html.GetResource("Layout_LayoutAdmin__line52")</a>
                    </div>
                </div>
                <!-- search form -->
                @*<form action="#" method="get" class="sidebar-form">
                        <div class="input-group">
                            <input type="text" name="q" class="form-control" placeholder="Search...">
                            <span class="input-group-btn">
                                <button type="submit" name="search" id="search-btn" class="btn btn-flat"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                    </form>*@
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu">
                    <li class="header">@Html.GetResource("Layout_LayoutAdmin__line67")</li>
                    <li class="treeview">
                        <a href="@Url.Action("ManageAgent", "Agency")">
                            <i class="fa fa-user"></i><span>&nbsp;Quản lý môi giới</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="@Url.Action("ManageProduct", "Agency")">
                            <i class="fa fa-home"></i><span>&nbsp;Quản lý sản phẩm</span>
                        </a>
                    </li>
                    <li class="treeview">
                        <a href="@Url.Action("ManageTransaction", "Agency")">
                            <i class="fa fa-file-text"></i><span>&nbsp;Quản lý giao dịch</span>
                        </a>
                    </li>
                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper">
            @RenderBody()
        </div>
    </div>
    @*<aside class="control-sidebar control-sidebar-dark">
        <div class="tab-content">
            <div class="tab-pane" id="control-sidebar-home-tab" style="display: block;">
                <h3 class="control-sidebar-heading">@Html.GetResource("Admin_ManageCity__line58")</h3>
                <ul class="control-sidebar-menu">
                    <li>
                        <a href="@Url.GoAccountLogout()">
                            <i class="fa fa-sign-out fa-fw"></i>
                            <strong>@Html.GetResource("Layout_LayoutAdmin__line156")</strong>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.GoAccountChangePassword()">
                            <i class="fa fa-lock fa-fw"></i>
                            <strong>@Html.GetResource("Layout_LayoutAdmin__line162")</strong>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </aside>*@
    <!-- /.control-sidebar -->
    <input id="layoutScripts" type="hidden" value="@Scripts.Url("~/Scripts/layoutadmin")" />
    @if (IsSectionDefined(@"Scripts"))
    {
    <div id="childSCripts" class="hidden">
            @RenderSection(@"Scripts", false)
        </div>
    }
    @Scripts.Render("~/Scripts/PreLoad")
</body>
</html>
