@using XT.Web.External

@{
    ViewBag.Title = "[Schedule] Quản lý danh sách thi";
    var title = ViewBag.Title;
    Layout = "~/Views/Layout/LayoutAdmin.cshtml";
}

<section class="content-header">
    <h1>@title<br />
        <br /><small>C1504L - Typography Techniques (1/5/2016 - 27/5/2016) - FC Đậu Ngọc Hà Dương</small>
    </h1>
    <ol class="breadcrumb low">
        <li><a href="@Url.GoAccountAdminFirstPage()"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("ManageSchedule_ClassSession")">Danh sách lịch học</a></li>
        <li class="active">C1504L - Typography Techniques</li>
    </ol>
</section>
<section class="content">
    <div class="row ajax-search-group">
        <div class="col-lg-12">
            <div class="Search_Result">
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Danh sách điểm thi</h3>
                        <div class="box-tools">
                            <button class="btn btn-warning" data-toggle="modal" data-target="#add-modal">
                                <i class="fa fa-plus fa-fw"></i>&nbsp;Khen thưởng mới
                            </button>
                        </div>
                    </div>
                    @Html.Partial("Search_Result/_partial_Search_ClassSession_Exam")
                </div>
            </div>
        </div>
    </div>
</section>