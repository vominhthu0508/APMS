@using XT.Web.External

@{
    ViewBag.Title = "[Academic] Quản lý skill của FC";
    var title = ViewBag.Title;
    Layout = "~/Views/Layout/LayoutAdmin.cshtml";

    var student_name = "Đậu Ngọc Hà Dương";
}

<section class="content-header">
    <h1>@title
        <small>@student_name</small>
    </h1>
    <ol class="breadcrumb low">
        <li><a href="@Url.GoAccountAdminFirstPage()"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("ManageAcademic_Faculty")">Danh sách FC</a></li>
        <li class="active">@student_name</li>
    </ol>
</section>
<section class="content">
    <div class="row ajax-search-group">
        <div class="col-md-12">
            <div class="Search_Result">
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Danh sách skill của FC</h3>
                        <div class="box-tools">
                            <button class="btn btn-warning" data-toggle="modal" data-target="#add-modal">
                                <i class="fa fa-plus fa-fw"></i>&nbsp;Skill mới
                            </button>
                        </div>
                    </div>
                    @Html.Partial("Search_Result/_partial_Search_Faculty_Skills")
                </div>
            </div>
        </div>
    </div>
</section>